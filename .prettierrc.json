{
    "plugins": [
        "./node_modules/prettier-plugin-multiline-arrays"
    ],
    "printWidth": 100,
    "singleQuote": true,
    "arrowParens": "avoid",
    "semi": false,
    "tabWidth": 2,
    "multilineArraysWrapThreshold": 10,
    "jsxSingleQuote": true,
    "prettier.bracketSpacing": true,
    "trailingComma": "none"
}